  "scope": "kchungradio",
  "builds": [
    {
      "src": "index.js",
      "use": "now-micro"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "GOOGLE_API_CLIENT_EMAIL": "@gcal-service-email",
