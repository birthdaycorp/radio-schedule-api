{
  "name": "kchungradio-schedule-api",
  "version": "1.1.0",
  "description": "KCHUNG Schedule API for http://www.kchungradio.org/schedule.html",
  "repository": "kchungradio/schedule-api",
  "author": "Tim Phillips",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^15.0.0",
    "micro": "^7.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "standard": "^8.6.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "start": "micro",
    "dev": "source ./lib/auth/env_secrets.sh && micro -p 1630"
  },
  "now": {
    "env": {
      "NODE_ENV": "production",
      "GOOGLE_API_CLIENT_EMAIL": "@kchung-gcal-service-email",
      "GOOGLE_API_PRIVATE_KEY_BASE64": "@kchung-gcal-private-key",
      "GOOGLE_CALENDAR_ID": "@kchung-gcal-calendar-id",
      "GOOGLE_USER_EMAIL": "@kchung-gcal-user-email",
      "TZ": "America/Los_Angeles"
    }
  }
}
